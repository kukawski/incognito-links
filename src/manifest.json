{
    "manifest_version": 2,
    "name": "__MSG_extensionName__",
    "version": "1.0",
    "description": "__MSG_extensionDescription__",
    "author": "Rafa≈Ç Kukawski",
    "permissions": [
        "storage"
    ],
    "default_locale": "en",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "icons": {
        "48": "icon.svg",
        "96": "icon.svg"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"],
          "all_frames": true
        }
    ],
    "options_ui": {
        "page": "settings.html",
        "browser_style": true
    }
}